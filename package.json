{
  "name": "strapi-plugin-iconify-picker",
  "version": "1.0.0",
  "keywords": [
    "strapi",
    "plugin",
    "v5",
    "iconify",
    "icons",
    "picker",
    "custom-field",
    "design-system"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@iconify/react": "^6.0.2",
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.33.0",
    "@strapi/typescript-utils": "^5.33.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.7.4",
    "react-intl": "^8.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.33.0",
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^8.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-iconify-picker",
    "displayName": "Iconify Picker",
    "description": "The ultimate icon selection experience for Strapi v5. Access 150,000+ icons with a premium, focused workflow."
  },
  "description": "The ultimate icon selection experience for Strapi v5. Access 150,000+ icons with a premium, focused workflow.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sunny-Pirate/strapi-plugin-iconify-picker.git"
  },
  "bugs": {
    "url": "https://github.com/Sunny-Pirate/strapi-plugin-iconify-picker/issues"
  },
  "homepage": "https://github.com/Sunny-Pirate/strapi-plugin-iconify-picker#readme",
  "author": "Luca Faccio <info@dreamlab.solutions>"
}